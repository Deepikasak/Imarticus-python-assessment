   "source": [
    "from itertools import accumulate\n",
    "import itertools as it\n",
    "b=[9,8,7,6,5]\n",
    "li=[]\n",
    "li=it.accumulate(list(enumerate(b)),lambda c,d:(d[0],(c[1]*(c[0]+1)+d[1])/(d[0]+1)))\n",
    "print(b)\n",
    "print(list(map(lambda x:x[1],li)))"
   ]
